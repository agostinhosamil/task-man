{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": [ "./*" ],
			"@styles": [
				"styles",
				"./components/@styles"
			],
			"@components": [
				"./components"
			],
			"@styles/*": [
				"styles/*",
				"./components/@styles/*"
			],
			"@components/*": [
				"./components/*"
			]
		}
	}
}